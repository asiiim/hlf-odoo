<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_partner_bank_form_inherit_bank_branch" model="ir.ui.view">
        <field name="name">Partner Bank Inherit Branch Form View</field>
        <field name="model">res.partner.bank</field>
        <field name="inherit_id" ref="base.view_partner_bank_form"/>
        <field name="arch" type="xml">
            <field name="bank_id" position="after">
                <field name="bank_branch"/>
            </field>
        </field>
    </record>
    <record id="view_partner_bank_tree_inherit_bank_branch" model="ir.ui.view">
        <field name="name">Partner Bank Inherit Branch Tree View</field>
        <field name="model">res.partner.bank</field>
        <field name="inherit_id" ref="base.view_partner_bank_tree"/>
        <field name="arch" type="xml">
            <field name="bank_name" position="after">
                <field name="bank_branch"/>
            </field>
        </field>
    </record>
</odoo>