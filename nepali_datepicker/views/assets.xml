<odoo>
	<template id="assets_nepali_datepicker" name="assets_nepali_datepicker" inherit_id="web.assets_backend">
		<xpath expr="." position="inside">
			<script type="text/javascript" src="/nepali_datepicker/static/lib/jquery.nepaliDatePicker.min.js"></script>
			<script type="text/javascript" src="/nepali_datepicker/static/src/nepali_calendar.js"></script>
		        <link rel="stylesheet" href="/nepali_datepicker/static/lib/nepaliDatePicker.min.css"/>
			<link rel="stylesheet" href="/nepali_datepicker/static/src/nepali_calendar.less"/>
		</xpath>
	</template>

	<template id="assets_common_inherited" name="assets_common_nepali_datepicker" inherit_id="web.assets_common">
		<xpath expr="." position="inside">
			<script type="text/javascript" src="/nepali_datepicker/static/src/widget_nepali_calendar.js"></script>
		</xpath>
	</template>
</odoo>
